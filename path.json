{
  "img":{
    "src":
    [
      "./client/path/to/img/directory/*.png",
      "./client/path/to/img/directory/*.jpg",
      "./client/path/to/img/directory/*.gif"
    ],
    "destination": "./path/of/destination"
  },

  "css":{
    "src": ["./client/path/to/css/directory/*.css"],
    "destination": "./path/of/destination",
    "rename": null
  },

  "js":{
    "src": ["./client/path/to/js/directory/*.js"],
    "destination": "./path/of/destination",
    "rename": null
  },

  "html":{
    "src": ["./client/path/to/html/directory/*.html"],
    "destination": "./path/of/destination",
    "rename": null
  }
}